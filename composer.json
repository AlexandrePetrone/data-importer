{
    "name": "iq2i/data-importer",
    "type": "library",
    "description": "A PHP library to easily manage and import large data file",
    "keywords": ["php", "file", "data", "import", "csv", "xml", "dto"],
    "homepage": "https://github.com/IQ2i/data-importer",
    "license": "MIT",
    "authors": [
        {
            "name": "Lo√Øc Sapone",
            "email": "loic@sapone.fr"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/filesystem": "^5.0 || ^6.0",
        "symfony/property-access": "^5.0 || ^6.0",
        "symfony/serializer": "^5.0 || ^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": { "IQ2i\\DataImporter\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "IQ2i\\DataImporter\\Tests\\": "Tests/" }
    }
}
